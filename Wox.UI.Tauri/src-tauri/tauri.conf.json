{
  "build": {
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build",
    "devPath": "http://localhost:1420/",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "woxuitauri",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.tauri.dev",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "transparent": true,
        "decorations": true,
        "alwaysOnTop": true,
        "skipTaskbar": true,
        "acceptFirstMouse": true,
        "title": "Wox",
        "width": 800,
        "maxHeight": 500
      }
    ]
  }
}
