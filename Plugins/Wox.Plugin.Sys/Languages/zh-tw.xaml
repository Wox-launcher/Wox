    <!--Dialogs-->
    <system:String x:Key="wox_plugin_sys_dlgtitle_success">成功</system:String>
    <system:String x:Key="wox_plugin_sys_dlgtext_all_settings_saved">所有 Wox 設置已保存</system:String>
