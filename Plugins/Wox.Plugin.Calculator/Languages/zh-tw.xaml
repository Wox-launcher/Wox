<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <system:String x:Key="wox_plugin_caculator_plugin_name">計算機</system:String>
    <system:String x:Key="wox_plugin_caculator_plugin_description">為 Wox 提供數學計算功能。(試著在 Wox 輸入 5*3-2)</system:String>
    <system:String x:Key="wox_plugin_calculator_not_a_number">結果非正常值 (NaN)</system:String>
    <system:String x:Key="wox_plugin_calculator_expression_not_complete">表達式錯誤或不完整（您是否忘記了一些括號？）</system:String>
    <system:String x:Key="wox_plugin_calculator_copy_number_to_clipboard">將此數值復製到剪貼板</system:String>
    <system:String x:Key="wox_plugin_calculator_output_decimal_seperator">小數點分隔符</system:String>
    <system:String x:Key="wox_plugin_calculator_output_decimal_seperator_help">要在輸出中使用的小數點分隔符。</system:String>
    <system:String x:Key="wox_plugin_calculator_decimal_seperator_use_system_locale">使用系統區域設置</system:String>
    <system:String x:Key="wox_plugin_calculator_decimal_seperator_comma">逗號 （，）</system:String>
    <system:String x:Key="wox_plugin_calculator_decimal_seperator_dot">點 （.）</system:String>
    <system:String x:Key="wox_plugin_calculator_max_decimal_places">最大小數位數</system:String>
</ResourceDictionary>
