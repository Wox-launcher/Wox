    <system:String x:Key="wox_plugin_calculator_output_decimal_seperator_help">O separador decimal para utilizar no resultado.</system:String>
    <system:String x:Key="wox_plugin_calculator_decimal_seperator_use_system_locale">Utilizar configuração do sistema</system:String>
    <system:String x:Key="wox_plugin_calculator_decimal_seperator_comma">Vírgula (,)</system:String>
